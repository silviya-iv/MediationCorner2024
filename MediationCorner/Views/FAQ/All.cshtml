@using MediationCorner.Core.Models;
@model List<FrequentlyAskedQuestionModel> 

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>FAQ</title>
    <!-- Add any necessary CSS styles or external CSS links here -->
</head>
<body>
    <h1>Frequently Asked Questions</h1>

    <ul>
        @foreach (var faq in Model) 
        {
            <li>
               
                <p><strong>Question:</strong> @faq.Question</p> 
                <p><strong>Answer:</strong> @faq.Answer</p> <!-- Display FAQ answer -->
            </li>
        }
    </ul>

    <!-- Add any necessary JavaScript scripts or external script links here -->
</body>
</html>